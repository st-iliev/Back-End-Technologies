function mathOperation(firstNumber , secondNumber, operation){
    let result ;
    switch(operation){
        case "+":
        result=(firstNumber+secondNumber);break;
        case "-":
        result=(firstNumber-secondNumber); break;
        case "*":
        result=(firstNumber*secondNumber); break;
        case "/":
        result=(firstNumber/secondNumber); break;
        case "%":
        result=(firstNumber%secondNumber); break;
        case "**":
        result=(firstNumber**secondNumber); break;
    }
    console.log(result);
}

mathOperation(1,2,'+')