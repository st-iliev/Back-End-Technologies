function solve(firstNumber,secondNumber){
    let result = 0;
    for(let i = firstNumber;i <= secondNumber; i++){
        if(i== secondNumber){
            console.log(i);
        }else{
            process.stdout.write(`${i} `);
        }
        result += i;
    }
    console.log(`Sum: ${result}`)
}

solve(5,10)